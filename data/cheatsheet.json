				}]
			},
			{
				"history": 
				[{
					"command" : "git log",
					"description": "Show all commits, newest first.",
					"tag": ["log"]
				},
				{
					"command" : "git log -p myfile",
					"description": "Show changes over time for myfile",
					"tag": ["log"]
				},
				{
					"command" : "git blame myfile",
					"description": "See who changed what/when in myfile",
					"tag": ["blame"]
				}]
			}			
